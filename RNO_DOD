#include <iostream>
using namespace std;
int main() 
{
	int t=0<t<100;
    int n1;
    cin>>t;
    do
    {
        int suma =0;
        cin>>n1;
        do
        {
            int a;
            cin>>a;
            suma+=a;
            n1--;
        }while(n1!=0);
        cout<<suma<< "\n";
        t--;
    }while(t!=0);
	return 0;
}
